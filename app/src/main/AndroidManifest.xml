<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lozyloop.pawtravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Screen.Register.Loading.LoadingActivity" />
        <activity android:name=".Screen.Register.RegisterActivity" />
        <activity android:name=".Screen.Register.SigninActivity" />
        <activity android:name=".Screen.Register.CreateActivity">
        </activity>
        <activity android:name=".Screen.Register.ResetActivity" />
        <activity android:name=".Screen.Handle.TravelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>